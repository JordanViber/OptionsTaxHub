# Database
DATABASE_URL=postgresql://user:password@localhost:5432/optionstaxhub

# API Keys
API_KEY_SECRET=your_secret_key_here

# CORS
FRONTEND_URL=http://localhost:3000

# Port (optional, defaults to 8080)
PORT=8080

# Push Notification Configuration
# Generate VAPID keys using: python3 -c "from pywebpush import vapid; print(vapid.Vapid().generate_keys())"
VAPID_PRIVATE_KEY=your_private_key_here
VAPID_PUBLIC_KEY=your_public_key_here
VAPID_CLAIM_EMAIL=mailto:your-email@example.com
