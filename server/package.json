{
  "name": "options-tax-hub-backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "python -m uvicorn main:app --host 0.0.0.0 --port 8080",
    "dev": "python -m uvicorn main:app --host 0.0.0.0 --port 8080 --reload",
    "test": "python -m pytest",
    "test:coverage": "python -m pytest --cov=. --cov-report=term-missing",
    "test:watch": "python -m pytest --watch"
  }
}
